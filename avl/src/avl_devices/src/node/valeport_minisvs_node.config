#===============================================================================
# Autonomous Vehicle Library
#
# Description: Configuration file for the Valeport miniSVS node.
#===============================================================================

# Sensor data output rate (1, 2, 4, 8, 16, 32, or 60 Hz)
output_rate: 2

# Serial port settings ---------------------------------------------------------
serial:

    # Serial port name the device is connected to
    port_name: "/dev/ttyS2"

    # Serial baud rate in bits per second
    baudrate: 19200

    # Serial read timeout in milliseconds
    read_timeout: 0
