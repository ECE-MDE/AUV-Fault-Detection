#===============================================================================
# Autonomous Vehicle Library
#
# Description: Configuration file for the Valeport miniCT node.
#===============================================================================

# Sensor data output rate (1, 2, 4 or 8 Hz)
output_rate: 1

# Serial port settings ---------------------------------------------------------
serial:

    # Serial port name the device is connected to
    port_name: "/dev/ttyS3"

    # Serial baud rate in bits per second
    baudrate: 19200

    # Serial read timeout in milliseconds
    read_timeout: 0
