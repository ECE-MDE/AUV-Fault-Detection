#===============================================================================
# Autonomous Vehicle Library
#
# Description: Configuration file for the Iridium modem node.
#===============================================================================

# Minimum signal strength for an SBD session attempt will be made
min_signal_strengh: 2

# Serial port settings
serial:

    # Serial port name the device is connected to
    port_name: "/dev/iridium"

    # Serial baud rate in bits per second
    baudrate: 19200

    # Serial read timeout in milliseconds
    read_timeout: 100
