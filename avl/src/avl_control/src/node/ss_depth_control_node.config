# Number of depth rate measurements to use in the moving average for smoothing
moving_avg_size: 10

# Maximum pitch angle output in degrees, saturated at +/- this value
max_pitch: 20.0

# Maximum pitch angle from integrator in degrees, saturated at +/- this value
